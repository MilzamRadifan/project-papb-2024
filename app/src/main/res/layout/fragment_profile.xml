<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileFragment"
    android:background="#000519"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PROFILE"
            android:textColor="#fff"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvIdPeminjaman"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textColor="#fff"
            app:layout_constraintEnd_toEndOf="@+id/textView"
            app:layout_constraintStart_toStartOf="@+id/textView"
            app:layout_constraintTop_toBottomOf="@+id/textView" />

        <TextView
            android:id="@+id/tvIdMovie"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textColor="#fff"
            app:layout_constraintEnd_toEndOf="@+id/tvIdPeminjaman"
            app:layout_constraintStart_toStartOf="@+id/tvIdPeminjaman"
            app:layout_constraintTop_toBottomOf="@+id/tvIdPeminjaman" />

        <TextView
            android:id="@+id/tvTanggalPeminjaman"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textColor="#fff"
            app:layout_constraintEnd_toEndOf="@+id/tvIdMovie"
            app:layout_constraintStart_toStartOf="@+id/tvIdMovie"
            app:layout_constraintTop_toBottomOf="@+id/tvIdMovie" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>